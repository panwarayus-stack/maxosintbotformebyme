{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "num.html", "use": "@vercel/static" },
    { "src": "osint.html", "use": "@vercel/static" },
    { "src": "api/fetchData.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/num", "dest": "/num.html" },
    { "src": "/osint", "dest": "/osint.html" },
    { "src": "/api/fetchData", "dest": "/api/fetchData.js" }
  ]
}
